stock IsARPName(Name[])
{
	new simb_, RP_Name[2][24];
	new playerid;
	for(new i = 0;i < strlen(Name);i ++)
	{
	    if(Name[i] == '_') simb_ ++;
	}
	if(simb_ != 1) return 0;
	sscanf(Name,"p<_>s[24]s[24]",RP_Name[0],RP_Name[1]);
	if(!IsAGoodName(RP_Name[0]) || !IsAGoodName(RP_Name[1])) return 0;
