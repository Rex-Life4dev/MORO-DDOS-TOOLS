			{
				b_info[b][b_mafia] = p_info[playerid][member];
				update_business_text(b);

				new query_string[228];
				format(query_string, 118, "UPDATE `businesses` SET `b_mafia`='%d' WHERE `b_id`='%d' LIMIT 1", b_info[b][b_mafia], b_info[b][b_id]);
