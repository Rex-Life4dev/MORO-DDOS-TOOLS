	if(pl_health + 65 > 160) set_health(playerid, 160);
	else set_health(playerid, pl_health + 65);

	GameTextForPlayer(playerid, "~g~+65HP", 2000, 1);

	me_action(playerid, "kotak pertolongan pertama bekas");
