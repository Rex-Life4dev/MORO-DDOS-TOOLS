					p_t_info[playerid][p_animation] = true;
					switch(random(3))
					{
						case 0:SetPlayerCheckpoint(playerid, 2654.0056, -2367.4011, 13.2651-0.5, 4.0);
						case 1:SetPlayerCheckpoint(playerid, 2646.1121, -2367.2539, 13.2651-0.5, 4.0);
