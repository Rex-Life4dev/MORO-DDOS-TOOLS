	SendClientMessage(targetid, col_gray, scm_string);

	format(scm_string, 144, "[A] %s[%d] memecat seorang pemain %s[%d] dari organisasi. Sebab: %s", p_info[playerid][name], playerid, p_info[targetid][name], targetid, reason);
