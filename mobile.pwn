find_available_spoiler_slot(modelid, stage, bool:type)
{
	if(type)
	{
		new point = 0;
		for(new i = 0; i <= 91; i ++)
		{
			if(modelid == legal_tuns[i][0])
			{
				point = i;
				break;
			}
		}
		if(point == 0) return false;
		for(new i = 0; i <= 19; i ++)
		{
			for(new j = 0; j <= 44; j ++)
			{
				if(i <= stage) continue;
		        if(legal_tuns[point][j] == spoiler_models[i]) return i + 1;
			}
		}
		for(new i = 0; i <= 19; i ++)
		{
	 		for(new j = 0; j <= 44; j ++)
			{
	       		if(legal_tuns[point][j] == spoiler_models[i]) return i + 1;
	       	}
		}
	}
	else
	{
		new point = 0;
		for(new i = 0; i <= 91; i ++)
		{
			if(modelid == legal_tuns[i][0])
			{
				point = i;
				break;
			}
		}
		if(point == 0) return false;
		for(new i = 19; i != 0; i --)
		{
			for(new j = 0; j <= 44; j ++)
			{
				if(i >= stage) continue;
		        if(legal_tuns[point][j] == spoiler_models[i]) return i + 1;
			}
		}
		for(new i = 19; i != 0; i --)
		{
	 		for(new j = 0; j <= 44; j ++)
			{
	       		if(legal_tuns[point][j] == spoiler_models[i]) return i + 1;
	       	}
		}
	}
	return 0;
}



